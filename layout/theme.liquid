    <script>
      var app = app || {
        data:{
          template:{{ template | json }},
          money_format: {{ shop.money_format | default:false | json }}
        }
      }
      function responsive_slick_slider(){
        {% if template == 'index' or template contains 'product' %}
        /*if ($(window).innerWidth()<= 992 && $(window).innerWidth() > 590 ) {*/
          // alert('hiii');
        /*Neil Where are you*/
          $('.slider').on('init', function(){
            $(this).fadeIn("slow");
          });
          $(".slider").slick({
            dots: true,
            infinite: true,
            slidesToShow: 6,
            slidesToScroll: 6,
            arrow: true,
            infinite: true,
            speed: 300,
            slidesToShow: 6,
            slidesToScroll: 6,
            responsive: [
              {
                breakpoint: 1024,
                settings: {
                  slidesToShow: 4,
                  slidesToScroll: 4,
                  infinite: true,
                  dots: true
                }
              },
              {
                breakpoint: 600,
                settings: {
                  slidesToShow: 3,
                  slidesToScroll: 3
                }
              },
              {
                breakpoint: 480,
                settings: {
                  slidesToShow: 2,
                  slidesToScroll: 2
                }
              }
              // You can unslick at a given breakpoint now by adding:
              // settings: "unslick"
              // instead of a settings object
            ]
          });
        /*
        }
        else if ($(window).innerWidth()<590 ) {
          // alert('hiii');
          $(".slider").slick({
            dots: true,
            infinite: true,
            slidesToShow: 2,
            slidesToScroll: 2,
            arrow: true
          });
        }
        else {
          //alert('hello');
          $(".slider").slick({
            dots: true,
            infinite: true,
            slidesToShow: 6,
            slidesToScroll: 6,
            arrow: true
          });
        }
        */
        {% endif %}
        {% if template == 'index' %}
        $(".owl").owlCarousel133({
          autoPlay : true,
          items : 7,
          itemsDesktop : [1199,5],
          itemsDesktopSmall : [980,4],
          itemsTablet: [700,3],
          itemsMobile : [400,2],
          slideSpeed : 4000,
          paginationSpeed : 4000,
          rewindSpeed : 4000,
          navigation : true,
          stopOnHover : true,
          pagination : false,
          scrollPerPage:true,
        });
        // just for vertical centering the brand icons.
        var _img_max_height = 0;
        $(".owl .owl-item .item-inner img").each(function(){
          if($(this).height() > _img_max_height)
            _img_max_height = $(this).height();
        });
        $(".owl .owl-item .item-inner").css({"line-height":_img_max_height+"px"});
        {% endif %}
      }
      $(document).ready(function(){	
        responsive_slick_slider();
      });
      /*
      $(window).resize(function(){
        responsive_slick_slider();
      });
      */

    </script>

